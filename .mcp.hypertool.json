{
  "mcpServers": {
    "logseq": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "mcp-logseq",
        "mcp-logseq"
      ],
      "env": {
        "LOGSEQ_API_TOKEN": "${LOGSEQ_API_TOKEN}",
        "LOGSEQ_API_URL": "http://127.0.0.1:12315"
      },
      "description": "LogSeq knowledge base integration",
      "disabled": false,
      "autoApprove": []
    },
    "grafana": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "mcp-grafana",
        "mcp-grafana"
      ],
      "env": {
        "GRAFANA_URL": "${GRAFANA_URL}",
        "GRAFANA_API_TOKEN": "${GRAFANA_API_TOKEN}"
      },
      "description": "Grafana observability dashboard integration",
      "disabled": false,
      "autoApprove": []
    }
  }
}
