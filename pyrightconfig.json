{
  "include": [
    "platform/*/src",
    "platform/*/tests",
    "apps/*/src"
  ],
  "exclude": [
    "platform/*/examples",
    "**/__pycache__",
    "**/*.pyc",
    "**/node_modules",
    "**/.venv",
    "**/venv"
  ],
  "extraPaths": [
    "platform/primitives/src",
    "platform/observability/src",
    "platform/agent-context/src",
    "platform/agent-coordination/src",
    "platform/kb-automation/src",
    "platform/documentation/src",
    "platform/integrations/src"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic"
}